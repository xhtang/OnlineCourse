    <servlet>
        <servlet-name>AddChapterServlet</servlet-name>
        <servlet-class>servlet.AddChapterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddChapterServlet</servlet-name>
        <url-pattern>/addChapter</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AddPointServlet</servlet-name>
        <servlet-class>servlet.AddPointServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddPointServlet</servlet-name>
        <url-pattern>/addPoint</url-pattern>
    </servlet-mapping>

    <servlet>
